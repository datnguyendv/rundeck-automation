{% include "job-base.j2" %}
    commands:
      - description: Add vault into UI
        {% include "clean-filter.j2" %}
        autoSecureInput: "{{ autoSecureInput | default("false") }}"
        passSecureInput: "{{ passSecureInput | default("false") }}"
        script: |
          export EXEC_LOCATION=$HOME/projects/$RD_JOB_PROJECT
          echo $EXEC_LOCATION
          ls $EXEC_LOCATION
          python3 $EXEC_LOCATION/vault-key-input.py -i $RD_OPTION_VAULTKEY
      {% include "approve-step.j2" %}
      - autoSecureInput: "false"
        description: Finished Output
        passSecureInput: "false"
        script: |-
          export EXEC_LOCATION=$HOME/projects/$RD_JOB_PROJECT
          python3 $EXEC_LOCATION/clean-job.py --job-href @data.href@ --path /tmp/$RD_JOB_ID/$RD_JOB_EXECUTIONUUID
          echo "Job finished successfully."
    keepgoing: false
    strategy: node-first
